{
  "name": "babel-plugin-transform-dev",
  "version": "1.1.1",
  "description": "A plugin for Babel v6 which replaces `__DEV__` with `'production' !== process.env.NODE_ENV`.",
  "main": "lib/index.js",
  "keywords": [
    "babel-plugin",
    "transform",
    "dev",
    "debug"
  ],
  "author": "Martin Andert",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinandert/babel-plugin-transform-dev.git"
  },
  "bugs": {
    "url": "https://github.com/martinandert/babel-plugin-transform-dev/issues"
  },
  "homepage": "https://github.com/martinandert/babel-plugin-transform-dev#readme",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "build": "babel src/ --out-dir lib/",
    "lint": "eslint src/",
    "pretest": "npm run lint",
    "test": "mocha",
    "prepublish": "npm run build",
    "preversion": "npm test",
    "postversion": "git push --tags origin HEAD:master",
    "release:breaking": "npm test && npm version major -m 'Release v%s' && npm publish",
    "release:feature": "npm test && npm version minor -m 'Release v%s' && npm publish",
    "release:fix": "npm test && npm version patch -m 'Release v%s' && npm publish"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.10.2",
    "mocha": "^2.3.4"
  }
}
